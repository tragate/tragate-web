<!DOCTYPE html>
<html>

<head>
    <!-- Google Tag Manager -->
    <script>
        (function (w, d, s, l, i) {
            w[l] = w[l] || []; w[l].push({
                'gtm.start':
                    new Date().getTime(), event: 'gtm.js'
            }); var f = d.getElementsByTagName(s)[0],
                j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''; j.async = true; j.src =
                    'https://www.googletagmanager.com/gtm.js?id=' + i + dl; f.parentNode.insertBefore(j, f);
        })(window, document, 'script', 'dataLayer', 'GTM-M7B7RMB');
    </script>
    <!-- End Google Tag Manager -->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TraGate</title>
    @if (Config.IsProduction() == false)
    {
        <meta name="robots" content="noindex, nofollow" />
    }
    @Html.Partial("_Styles")
    @if (IsSectionDefined("Styles"))
    {
        RenderSection("Styles", required: false);
    }

</head>
<body class="woocommerce-active left-sidebar page-template-template-homepage-v5">
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.ht...7RMB" ;
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->
    <div id="page" class="hfeed site">
        @Html.Partial("_CompanyHeader")
        <div id="content" class="site-content" tabindex="-1">
            <div class="col-full">
                <div class="row">
                    <nav class="woocommerce-breadcrumb">
                        <span class="delimiter">
                            <i class="fa fa-home"></i>
                        </span>
                        <a asp-action="index" asp-controller="home">TraGate</a>
                        <span class="delimiter">
                            <i class="tm tm-breadcrumbs-arrow-right"></i>
                        </span>
                        <a asp-action="profile" asp-controller="company">Company Name Company Name</a>
                        <span class="delimiter">
                            <i class="tm tm-breadcrumbs-arrow-right"></i>
                        </span>
                        Products  asdasdsds
                    </nav>
                    <div id="primary" class="content-area" style="background: #f9f9f9;">
                        <main id="main" class="site-main">
                            @RenderBody()
                        </main>
                    </div>
                    @Html.Partial("_CompanyProductsNavigation")
                </div>
            </div>
        </div>
        @Html.Partial("_Footer")
    </div>
    @Html.Partial("_Scripts")
    @RenderSection("Scripts", required: false)
</body>
</html>